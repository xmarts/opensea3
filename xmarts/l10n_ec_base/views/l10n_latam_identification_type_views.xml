<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- L10N LATAM IDENTIFICATION TYPE TREE -->
    <record id="view_l10n_latam_identification_type_code_tree" model="ir.ui.view">
        <field name="name">l10n.latam.identification.type.code.tree</field>
        <field name="model">l10n_latam.identification.type</field>
        <field name="inherit_id" ref="l10n_latam_base.view_l10n_latam_identification_type_tree"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='name']" position="before">
                <field name="code"/>
            </xpath>
        </field>
    </record>

</odoo>
