<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_account_report_tax_wizard_form" model="ir.ui.view">
        <field name="name">view.account.report.tax.wizard.form</field>
        <field name="model">account.report.tax.wizard</field>
        <field name="arch" type="xml">
            <form string="!03-104 Reports">
                <group>
                    <group>
                        <field name="date_start" required="1" class="oe_inline"/>
                    </group>
                    <group>
                        <field name="date_end" required="1" class="oe_inline"/>
                    </group>
                </group>
                <footer>
                    <button name="action_print" default_focus="1" string="Print Report" icon="fa-print" type="object"/>
                    <button special="cancel" string="Cancel" type="object"/>
                </footer>
            </form>
        </field>
    </record>

    <record id="action_report_tax_form_open" model="ir.actions.act_window">
        <field name="name">Declaraci√≥n de Impuestos</field>
        <field name="res_model">account.report.tax.wizard</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <menuitem action="action_report_tax_form_open"
              id="menu_action_tax_report"
              parent="menu_sri_reporting"
              sequence="200"/>


