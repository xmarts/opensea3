<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="view_res_bank_form" model="ir.ui.view">
        <field name="name">Res bank form Pichincha</field>
        <field name="model">res.bank</field>
        <field name="inherit_id" ref="base.view_res_bank_form"/>
        <field name="arch" type="xml">
            <field name="bic" position="after">
                <field name="pichincha_bank_code"/>
                <field name="guayaquil_bank_code"/>
            </field>
        </field>
    </record>

    <record id="view_res_bank_tree" model="ir.ui.view">
        <field name="name">Res bank tree Pichincha</field>
        <field name="model">res.bank</field>
        <field name="inherit_id" ref="base.view_res_bank_tree"/>
        <field name="arch" type="xml">
            <field name="bic" position="after">
                <field name="pichincha_bank_code"/>
                <field name="guayaquil_bank_code"/>
            </field>
        </field>
    </record>


</odoo>
